PATCH /orders/{id} Update an order
Response Class
ModelModel Schema
PatchOrderRequest {
user_id (integer, optional),
line_items (array[LineItemPatch], optional),
discount (DiscountPatch, optional),
deleted_line_items (array[LineItemPatch], optional),
requesting_mac_address (string, optional),
closed (Object, optional)
}
LineItemPatch {
number (string, optional),
catalog_item_id (integer, optional),
quantity (number, optional),
notes (string, optional),
sent_to_kitchen (Object, optional),
price_override (number, optional),
tax_reason (string, optional),
user_id (integer, optional),
discount (DiscountPatch, optional),
line_item_modifiers (array[LineItemModifierPatch], optional),
deleted (boolean, optional),
dirty (boolean, optional),
course (string, optional),
_taxable (Object, optional)
}
DiscountPatch {
order_id (integer, optional),
line_item_id (integer, optional),
name (string, optional),
amount (number, optional),
percentage (number, optional),
description (string, optional),
user_id (integer, optional),
discount_type_id (integer, optional)
}
LineItemModifierPatch {
line_item_id (integer, optional),
catalog_item_modifier_id (integer, optional),
quantity (number, optional),
notes (string, optional),
deleted (boolean, optional),
dirty (boolean, optional)
}

